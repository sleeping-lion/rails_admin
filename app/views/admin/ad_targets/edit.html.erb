<%= form_for([@ad_schedule.ad,@ad_schedule,@ad_target]) do |f| %>
 	<div class="field">
		<%= f.label :target_id,t(:label_target) %>
		<%= f.select :target_id, options_from_collection_for_select(Target.find(:all,:conditions=>{:flag=>true}), 'id', 'title',@ad_target.target_id) %>
 	</div>
 	<div class="actions">
 		<%= f.submit :class=>"btn btn-primary" %>
 	</div>
<% end %>