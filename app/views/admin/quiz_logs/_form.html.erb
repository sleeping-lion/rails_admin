<%= form_for [:admin,@quiz_log] do |f| %>
<% if @quiz_log.errors.any? %>
<div id="error_explanation" class="alert alert-danger">
	<a href="#" class="close" data-dismiss="alert">&times;</a>
	<h2><%= pluralize(@quiz_log.errors.count, "error") %> prohibited this point from being saved:</h2>
	<ul>
		<% @quiz_log.errors.full_messages.each do |msg| %>
		<li>
			<%= msg %>
		</li>
		<% end %>
	</ul>
</div>
<% end %>

<div class="form-group">
	<%= f.label :quiz_id %>
	<%= f.number_field :quiz_id,:class=>"form-control",:required=>"required" %>
	<%= link_to t(:quiz_id_find),'/admin/quizzes/quiz_id_select',:id=>'quiz_id_select',:title=>t(:quiz_id_find),:target=>'_blank',:class=>'btn btn-default modal_link',:'data-target'=>"#modal",:'data-toggle'=>"modal",:style=>'vertical-align:top' %>
</div>
<div class="form-group">
	<%= f.label :quiz_answer_id %>
	<%= f.number_field :quiz_answer_id,:class=>"form-control",:required=>"required" %>
	<%= link_to t(:quiz_answer_id_find),'/admin/users/user_id_select',:id=>'user_id_select',:title=>t(:quiz_answer_id_find),:target=>'_blank',:class=>'btn btn-default modal_link',:'data-target'=>"#modal",:'data-toggle'=>"modal",:style=>'vertical-align:top' %>
</div>
<div class="form-group">
	<%= f.label :user_id %>
	<%= f.number_field :user_id,:class=>"form-control",:required=>"required" %>
	<%= link_to t(:user_id_find),'/admin/users/user_id_select',:id=>'user_id_select',:title=>t(:user_id_find),:target=>'_blank',:class=>'btn btn-default modal_link',:'data-target'=>"#modal",:'data-toggle'=>"modal",:style=>'vertical-align:top' %>
</div>
<div class="form-group actions">
	<%= f.submit :class=>"btn btn-primary" %>
</div>
<% end %>
