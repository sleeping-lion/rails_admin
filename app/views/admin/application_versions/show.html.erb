<% if notice %>
<%= render :partial => 'layouts/notice',:locals=>{:notice=>notice} %>
<% end %>
<div class="sl_show">
	<p>
		<b><%=t(:label_version) %></b>
		<%= @application_version.client_version %>
	</p>
	<p>
		<b><%=t(:label_version_name) %></b>
		<%= @application_version.version_name %>
	</p>
	<p>
		<b><%=t(:label_state) %></b>
		<%= @application_version.state %>
	</p>
	<p>
		<b><%=t(:label_update_date)%></b>
		<%= @application_version.update_date %>
	</p>
	<p>
		<b><%=t(:label_apk_file) %></b>
		<%= @application_version.apk_file %>
	</p>
	<p>
		<b><%=t(:label_contents) %></b>
		<%= @application_version.contents %>
	</p>
  <p>
  	<b><%=t(:label_created_at) %></b>
  	<%= @application_version.created_at %>
  </p>
  <p>
  	<b><%=t(:label_updated_at) %></b>
  	<%= @application_version.updated_at %>
  </p>
</div>

<% if can? :create, @application_version %>
<%= link_to t(:link_edit), edit_admin_application_version_path(@application_version), class: "btn" %>
<% end %>
<%= link_to t(:link_list), admin_application_versions_path, class: "btn" %>
