<?xml version="1.0"?>
<Workbook xmlns="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:o="urn:schemas-microsoft-com:office:office"
  xmlns:x="urn:schemas-microsoft-com:office:excel"
  xmlns:ss="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:html="http://www.w3.org/TR/REC-html40">
  <Worksheet ss:Name="Sheet1">
    <Table>
      <Row>
        <Cell><Data ss:Type="String">Store good</Data></Cell>
        <Cell><Data ss:Type="String">User</Data></Cell>
        <Cell><Data ss:Type="String">Amount</Data></Cell>
        <Cell><Data ss:Type="String">Customer tel</Data></Cell>
        <Cell><Data ss:Type="String">Delivery tel</Data></Cell>
        <Cell><Data ss:Type="String">Delivery zipcode</Data></Cell>
        <Cell><Data ss:Type="String">Delivery address 1</Data></Cell>
        <Cell><Data ss:Type="String">Delivery address 2</Data></Cell>
        <Cell><Data ss:Type="String">Delivery note</Data></Cell>                
        <Cell><Data ss:Type="String">Status</Data></Cell>
        <Cell><Data ss:Type="String">Shipping method</Data></Cell>
        <Cell><Data ss:Type="String">Shipping track number</Data></Cell>
        <Cell><Data ss:Type="String">Shipping time</Data></Cell>
        <Cell><Data ss:Type="String">Shipping flag</Data></Cell>                
        <Cell><Data ss:Type="String">Return reason</Data></Cell>
        <Cell><Data ss:Type="String">Return time</Data></Cell>
        <Cell><Data ss:Type="String">Return flag</Data></Cell>
        <Cell><Data ss:Type="String"><%=t(:label_created_at) %></Data></Cell>
      </Row>
      <% @store_orders.each do |store_order| %>
      <Row>
        <Cell><Data ss:Type="Number"><%= store_order.store_good_id %></Data></Cell>
        <Cell><Data ss:Type="String"><%= store_order.user_id %></Data></Cell>
        <Cell><Data ss:Type="Number"><%= store_order.amount %></Data></Cell>
        <Cell><Data ss:Type="String"><%= store_order.customer_tel %></Data></Cell>
        <Cell><Data ss:Type="Number"><%= store_order.delivery_tel %></Data></Cell>
        <Cell><Data ss:Type="String"><%= store_order.delivery_zipcode %></Data></Cell>
        <Cell><Data ss:Type="Number"><%= store_order.delivery_address_1 %></Data></Cell>
        <Cell><Data ss:Type="String"><%= store_order.delivery_address_2 %></Data></Cell>
        <Cell><Data ss:Type="Number"><%= store_order.delivery_note %></Data></Cell>
        <Cell><Data ss:Type="String"><%= store_order.status %></Data></Cell>
        <Cell><Data ss:Type="Number"><%= store_order.shipping_method %></Data></Cell>
        <Cell><Data ss:Type="String"><%= store_order.shipping_track_number %></Data></Cell>
        <Cell><Data ss:Type="Number"><%= store_order.shipping_time %></Data></Cell>
        <Cell><Data ss:Type="String"><%= store_order.shipping_flag %></Data></Cell>
        <Cell><Data ss:Type="Number"><%= store_order.return_reason %></Data></Cell>
        <Cell><Data ss:Type="String"><%= store_order.return_time %></Data></Cell>
        <Cell><Data ss:Type="String"><%= flagToString(store_order.flag,true) %></Data></Cell>
        <Cell><Data ss:Type="String"><%= store_order.created_at %></Data></Cell>
      </Row>
      <% end %>
    </Table>
  </Worksheet>
</Workbook>