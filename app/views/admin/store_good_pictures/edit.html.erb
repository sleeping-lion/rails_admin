<%= form_for([@store_good_picture.store_good,@store_good_picture]) do |f| %>
	<%= f.hidden_field :filename,:value=>'asdgsadg' %>
	<div class="form-group">
    <%= f.label :title  %>
    <%= f.text_field :title %>
  </div>
	<div class="form-group">
 		<%= f.label :photo %>
 		<%= f.file_field :photo %>
 		<%= f.hidden_field :photo_cache %>
 	</div>
	<div class="form-group">
 		<%= f.label :flag %>
 		<%= f.check_box :flag %>
 	</div>
	<div class="form-group actions">
 		<%= f.submit :class=>"btn btn-primary" %>
 	</div>
<% end %>
