<%= form_for([@store_good,StoreGoodPicture.new]) do |f| %>
	<%= f.hidden_field :filename,:value=>'asdgsadg' %>
  <div class="field">
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>
 	<div class="field">
 		<%= f.label :photo %>
 		<%= f.file_field :photo,:required=>"required" %>
 		<%= f.hidden_field :photo_cache %>
 	</div>
 	<div class="field">
 		<%= f.label :flag %>
 		<%= f.check_box :flag %>
 	</div>
 	<div class="actions">
 		<%= f.submit :class=>"btn btn-primary" %>
 	</div>
<% end %>
